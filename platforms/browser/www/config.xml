<?xml version='1.0' encoding='utf-8'?>
<widget id="com.phonegap.dongrigorio.mypraktic" version="0.2.6" versionCode="0.2.6.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Моя практика</name>
    <description>
        Программа для аккуратных практикунов.
    </description>
    <author email="2dongrigorio@gmail.com" href="http://geo-format.ru">
        Don Grigorio
    </author>
    <content src="index.html" />
    <access origin="*" />
